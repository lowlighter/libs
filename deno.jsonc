    "unified": "npm:unified@11",
    "hast": "npm:@types/hast@3",
    "hastscript": "npm:hastscript@9",
    "unist-util-visit": "npm:unist-util-visit@5",
    "rehype-slug": "npm:rehype-slug@6",
    "rehype-autolink-headings": "npm:rehype-autolink-headings@7",
