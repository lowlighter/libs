  "tasks": {
    "clean": "deno eval 'import {fromFileUrl} from \"jsr:@std/path\";[...await Array.fromAsync(Deno.readDir(`.`))].filter(entry => entry.isDirectory).map(entry => fromFileUrl(new URL(entry.name, import.meta.url))).filter(path => path.includes(`/libs/std/`)).map(path => (console.log(`cleaning: ${path}`), Deno.removeSync(path, {recursive:true})))'",
    "build": "deno task clean && deno run --allow-read --allow-net=jsr.io,api.jsr.io --allow-write --allow-run=deno jsr:@libs/bundle@12/ts/cli/mirror --scope std --packages-exclude internal --config deno.jsonc --mod --patches patches.yml && deno fmt",
